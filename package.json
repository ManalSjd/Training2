    "@nebular/security": "8.0.0",
    "@nebular/theme": "8.0.0",
    "@swimlane/ngx-charts": "^19.0.1",
    "angular2-chartjs": "0.5.1",
    "bootstrap": "5.1.0",
    "chart.js": "3.5.1",
    "ckeditor": "4.7.3",
    "classlist.js": "1.1.20150312",
    "core-js": "3.16.3",
